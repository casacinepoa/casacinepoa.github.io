{
  "actions" : {
    "build" : {
      "enabled" : true,
      "script" : "#!\/bin\/sh\nbundler update"
    },
    "clean" : {
      "enabled" : true,
      "script" : "#!\/bin\/sh\nbundle exec jekyll clean"
    },
    "run" : {
      "enabled" : true,
      "script" : "#!\/bin\/sh\nbundle exec jekyll serve --livereload"
    }
  },
  "buildBeforeRunning" : true,
  "environment" : {
    "JEKYLL_ENV" : "development"
  },
  "openLogOnRun" : "start"
}
